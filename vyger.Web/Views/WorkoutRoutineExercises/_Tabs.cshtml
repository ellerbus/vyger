@model WorkoutRoutine

<div class="form-group">
  <div class="col-lg-12 col-md-12">
    <ul class="nav nav-tabs">
      @for (var w = 0; w < Model.Weeks; w++)
      {
        <li class="nav-item">
          <a class="nav-link @(w + 1 == Model.RoutineExercises.WeekId ? "active" : "")"
             href="@Url.Action(MVC.WorkoutRoutineExercises.Index(Model.RoutineId, w+1))">Week@(w + 1)</a>
          </li>
        }
    </ul>
  </div>
</div>
