@model WorkoutPlanExercise

<tr>
  @using (Html.BeginFieldScope(Model, "PlanExercises"))
  {
    <td>
      @Model.Exercise.Group.Name
    </td>
    <td>
      @Html.HiddenFor(m => m.ExerciseId)
      @Model.Exercise.Category.Name
      @Model.Exercise.Name
    </td>
    <td>
      @Html.TextBoxFor(m => m.Weight, new { @class = "form-control form-control-sm", style = "font-family: 'Courier New', Courier, monospace" })
      @Html.ValidationMessageFor(m => m.Weight, "", new { @class = "form-text text-danger", style = "font-family: 'Courier New', Courier, monospace" })
    </td>
    <td>
      @Html.TextBoxFor(m => m.Reps, new { @class = "form-control form-control-sm", style = "font-family: 'Courier New', Courier, monospace" })
      @Html.ValidationMessageFor(m => m.Reps, "", new { @class = "form-text text-danger", style = "font-family: 'Courier New', Courier, monospace" })
    </td>
    <td>
      @Html.TextBoxFor(m => m.Pullback, new { @class = "form-control form-control-sm", style = "font-family: 'Courier New', Courier, monospace" })
      @Html.ValidationMessageFor(m => m.Pullback, "", new { @class = "form-text text-danger", style = "font-family: 'Courier New', Courier, monospace" })
    </td>
  }
</tr>